{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/sdr.html",
      "destination": "/sdr-sales-training/",
      "permanent": true
    },
    {
      "source": "/ae.html",
      "destination": "/account-executive-training/",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about/",
      "permanent": true
    },
    {
      "source": "/resources.html",
      "destination": "/resources/",
      "permanent": true
    },
    {
      "source": "/blog.html",
      "destination": "/blog/",
      "permanent": true
    },
    {
      "source": "/blog-4-life-changing-benefits-cold-calling.html",
      "destination": "/blog/cold-calling-benefits/",
      "permanent": true
    },
    {
      "source": "/blog-overcome-call-reluctance.html",
      "destination": "/blog/overcome-call-reluctance/",
      "permanent": true
    },
    {
      "source": "/blog-stop-chasing-vanity-metrics.html",
      "destination": "/blog/stop-chasing-vanity-metrics/",
      "permanent": true
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy/",
      "permanent": true
    },
    {
      "source": "/terms.html",
      "destination": "/terms/",
      "permanent": true
    },
    {
      "source": "/:page.html",
      "destination": "/:page/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|png|gif|svg|ico|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000, immutable"
        }
      ]
    }
  ]
}
